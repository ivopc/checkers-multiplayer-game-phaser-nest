{
  "name": "blaze-1-bet-api",
  "version": "1.0.0",
  "description": "Arquivos que criam um back-end para o projeto Blaze 1 Bet",
  "main": "./src/main.ts",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Deibson Cogo",
    "email": "deibsoncogo@outlook.com",
    "url": "https://github.com/deibsoncogo/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jonatank28/BLAZE-1.BET-Back"
  },
  "keywords": [
    "blaze",
    "bet"
  ],
  "engines": {
    "node": ">=18"
  },
  "prisma": {
    "schema": "./src/infrastructure/database/schema.prisma",
    "seed": "ts-node ./src/infrastructure/database/seed.ts"
  },
  "scripts": {
    "reset": "prisma migrate reset -f",
    "migrate": "prisma migrate dev",
    "migrate:create": "prisma migrate dev --create-only",
    "migrate:deploy": "prisma migrate deploy",
    "studio": "prisma studio",
    "generate": "prisma generate",
    "seed": "npx prisma db seed",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node ./dist/src/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.ts",
    "test:e2e:cov": "vitest run --coverage --config ./vitest.config.e2e.ts",
    "error": "tsc --noEmit",
    "lint": "eslint . --config .eslintrc.json --ext ts,tsx",
    "lint-fix": "eslint . --config .eslintrc.json --ext ts,tsx --fix",
    "seed:prod": "node ./dist/src/infrastructure/database/seed.js"
  },
  "dependencies": {
    "@google-cloud/recaptcha-enterprise": "5.0.0",
    "@nestjs/bull": "10.0.1",
    "@nestjs/cli": "10.0.0",
    "@nestjs/common": "10.0.0",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.0.0",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/platform-express": "10.0.0",
    "@nestjs/swagger": "7.1.14",
    "@nestjs/throttler": "5.0.1",
    "@nestjs/platform-socket.io": "^10.2.8",
    "@nestjs/websockets": "^10.2.8",
    "@prisma/client": "5.3.1",
    "@sentry/node": "7.81.1",
    "@travelerdev/nestjs-sentry": "4.2.1",
    "aws-sdk": "2.1490.0",
    "axios": "1.5.1",
    "bcryptjs": "2.4.3",
    "bull": "4.11.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "crypto-js": "4.2.0",
    "dayjs": "1.11.10",
    "handlebars": "4.7.8",
    "mersenne-twister": "1.1.0",
    "random-js": "2.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1",
    "ts-node": "10.9.1",
    "unplugin-swc": "1.4.2",
    "vitest": "0.34.5",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@faker-js/faker": "8.1.0",
    "@nestjs/schematics": "10.0.0",
    "@nestjs/testing": "10.0.0",
    "@swc/core": "1.3.90",
    "@types/bcryptjs": "2.4.4",
    "@types/express": "4.17.17",
    "@types/multer": "1.4.9",
    "@types/node": "20.3.1",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@typescript-eslint/parser": "6.7.3",
    "@vitest/coverage-v8": "0.34.5",
    "eslint": "8.50.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import-helpers": "1.3.1",
    "eslint-plugin-prettier": "5.0.1",
    "prettier": "3.0.3",
    "prisma": "5.3.1",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "ts-loader": "9.4.3",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "vite-tsconfig-paths": "4.2.1"
  }
}
